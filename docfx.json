{
  "metadata": [
    {
      "src": [
        {
          "files": [ "LtsvHelper.sln" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "cwd": "obj/api",
        "dest": "api"
      }
    ],
    "resource": [
    ],
    "globalMetadata": {
      "_appTitle": "LtsvHelper",
      "_enableSearch": true
    },
    "dest": "docs"
  }
}
